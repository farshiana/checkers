{
	"globals": {
		"ENV": "prod"
	},
	"moduleNameMapper": {
		"^Source(.*)$"   : "<rootDir>/../src$1",
		"^Models(.*)$"   : "<rootDir>/../src/models$1",
		"^Helpers(.*)$"  : "<rootDir>/../src/helpers$1",
		"^Constants(.*)$": "<rootDir>/../src/constants$1"
	},
	"setupFiles": ["./jest.config.js"],
	"verbose": true,
	"moduleDirectories": ["node_modules", "src"]
}